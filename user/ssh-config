Host stash.cirb.lan
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  IdentityFile ~/.ssh/cirb_rsa
  PreferredAuthentications publickey

Host *.github.com
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  PreferredAuthentications publickey

Host github.com
  IdentityFile ~/.ssh/cirb_github_rsa

Host mygithub.com
  Include config_mygithub